/*右侧栏主盒子*/

.container {
  position: relative;
  /*宽度占满，高度占满，并使用百分比布局*/
  width: 100%;
  height: 100%;
  padding: 10px;
  box-sizing: border-box;
  background: #fff;
  text-align: center;
}

.container image {
  /*被图片设置宽高*/
  width: 300px;
  margin-top: 15px;
  background: #f5f5f5;
}

.container .head {
  font-size: 50px;
  color: red;
}

.container .wechat {
  font-size: 25px;
  color: red;
}

.container .detail {
  font-size: 20px;
  color: black;
  text-align: left;
}
